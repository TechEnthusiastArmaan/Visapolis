{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
        "@/*": ["src/*"],
      "@/lib/*": ["src/lib/*"],
      "@/models/*": ["src/models/*"],
      "@/components/*": ["src/app/(public)/components/*"],
      "@/actions/*": ["src/app/(public)/actions/*"]
    }
  },
  "exclude": ["node_modules"]
}